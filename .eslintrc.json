{
    "env": {
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
        "@typescript-eslint"
    ],
      "rules": { 
          "no-console": "off",
          "linebreak-style": "off",
          "no-use-before-define" : "off",
          "@typescript-eslint/no-use-before-define": "warn",
          "import/no-unresolved": "off",
          "import/extensions": "off",
          "camelcase":"off"
    }
}
